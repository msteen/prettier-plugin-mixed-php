{
  "exclude": ["test/**/*.ts"],
  "compilerOptions": {
    "alwaysStrict": true,
    "declaration": false,
    "declarationMap": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "outDir": "lib",
    "rootDir": "src",
    "sourceMap": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "target": "ES2020"
  }
}
